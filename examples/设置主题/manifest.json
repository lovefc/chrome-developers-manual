{
   // 配置文件版本,以后只支持3了
   "manifest_version": 3,
   // 扩展名称
   "name": "我的主题",
   // 扩展介绍
   "description": "一个自己定义的主题",
   // 扩展版本  
   "version": "1.0",
   // 主题设置
   "theme": {
      "colors": {
         "bookmark_text": [255,255,255],
         "frame": [22,36,41],
         "ntp_background": [18,36,42],
         "tab_background_text": [255,255,255],
         "tab_text": [255,255,255],
         "toolbar": [45,71,84]
      },
      "images": {
         "theme_ntp_background": "themes/theme_tab_background.jpg"
      },
      "properties": {
         "ntp_background_alignment": "bottom",
         "ntp_background_repeat": "repeat-x",
         "ntp_logo_alternate": 1
      },
      "tints": {
         "buttons": [1,1,1],
         "frame_inactive": [-1,-1,-1],
         "frame_incognito": [1,1,1],
         "frame_incognito_inactive": [-1,-1,-1]
      }
   }
}